package com.springlec.base0701.dto;

import java.sql.Timestamp;

public class BDto {

	
	//Field
	int bId;
	String bName;
	String bTitle;
	String bContent;
	Timestamp bDate;
	
	//Constructor
	public BDto() {
		// TODO Auto-generated constructor stub
	}

	public BDto(int bId, String bName, String bTitle, String bContent, Timestamp bDate) {
		super();
		this.bId = bId;
		this.bName = bName;
		this.bTitle = bTitle;
		this.bContent = bContent;
		this.bDate = bDate;
	}
	
	
	
	
}//-----
